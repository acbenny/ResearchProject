CREATE CLASS Orders EXTENDS V;
CREATE SEQUENCE svcIdSeq TYPE ORDERED;
CREATE SEQUENCE ordIdSeq TYPE ORDERED;

CREATE PROPERTY Orders.order_id LONG (MANDATORY TRUE, NOTNULL TRUE, DEFAULT "sequence('ordIdSeq').next()");
CREATE PROPERTY Orders.service_id STRING (MANDATORY TRUE, NOTNULL TRUE);

CREATE INDEX Orders.order_id on Orders(order_id) UNIQUE;

CREATE CLASS NextOrder EXTENDS E;

CREATE PROPERTY NextOrder.out LINK Orders;
CREATE PROPERTY NextOrder.in LINK Orders;