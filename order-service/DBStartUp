CREATE CLASS Orders EXTENDS V;
CREATE SEQUENCE svcIdSeq TYPE ORDERED;
CREATE SEQUENCE ordIdSeq TYPE ORDERED START 100;

CREATE PROPERTY Orders.orderId LONG (MANDATORY TRUE, NOTNULL TRUE, DEFAULT "sequence('ordIdSeq').next()");
CREATE PROPERTY Orders.serviceId STRING (MANDATORY TRUE, NOTNULL TRUE);
CREATE PROPERTY Orders.status STRING;
CREATE PROPERTY Orders.vpnName STRING;

CREATE INDEX Orders.orderId on Orders(orderId) UNIQUE;

CREATE CLASS NextOrder EXTENDS E;

CREATE PROPERTY NextOrder.out LINK Orders;
CREATE PROPERTY NextOrder.in LINK Orders; 